    agent any 
    
    stages {
    	
    	stage("build") {
    	     step {
    	     	echo "Building stage...";
    	     }
    	}
    	
    	stage("test") {
    	
    	     step {
    	     	echo "Testing stage...";    
    	     }

    	     
    	    
    	}
    	
    	stage("deploy") {
    	
    	     step {
    	     	echo "Deploying stage...";
    	     }
    	    
    	}
    	

        
        
    }

    
    
}
